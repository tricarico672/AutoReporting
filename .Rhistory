import pyarrow.dataset as ds
import pandas as pd
import plotly.express as px
import plotly.io as pio
pio.renderers.default = "plotly_mimetype+notebook_connected"
df = ds.dataset("~/Desktop/Projects/R_projects/test_reporting/data/parquet", format="parquet").to_table().to_pandas()
#df = pd.read_csv("~/Desktop/Projects/R_projects/test_reporting/data/raw/2024_spotify-most-streamed.csv")
fig = px.scatter(df, x="YouTube Likes", y="TikTok Likes",
marginal_y="violin", marginal_x="box",
trendline="ols", template="simple_white")
fig.show()
